a=list(map(int,input().split()))
if a[0]<1 or a[1]<1 or a[2]<1:
    print("invalid")
else:    
    if a[0]==a[1] and a[1]==a[2] and a[3]==a[1]:
        print("equlatoral triangle");
    elif (a[0]==a[1]) or (a[1]==a[2]) or (a[2]==a[0]):
        print("isolated triangle")
    else:
        print("sclar triangle")
